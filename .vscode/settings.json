{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true,
        "**/*.backup": true,
        "**/*-backup.*": true,
        "**/*-new.*": true,
        "**/*-old.*": true,
        "**/*.tmp": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/dist/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true,
        "**/*.backup": true,
        "**/*-backup.*": true,
        "**/*-new.*": true,
        "**/*-old.*": true,
        "**/*.tmp": true
    }
}
